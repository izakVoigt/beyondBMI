{
  "name": "beyond",
  "version": "0.0.1",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=24.12.0",
    "npm": ">=11.6.2"
  },
  "scripts": {
    "build": "npx nx run-many --target=build --parallel=5",
    "serve": "bash -lc 'set -e; docker compose up --build -d; trap \"docker compose stop\" EXIT INT TERM; npx nx run-many --target=serve --parallel=5'",
    "lint:check": "npx nx run-many --target=lint --parallel=5",
    "lint:fix": "npx nx run-many --target=lint:fix --parallel=5",
    "format:check": "npx nx format:check --all --parallel=5",
    "format:fix": "npx nx format:write --all --parallel=5",
    "format:uncommitted": "npx nx format:write --uncommitted --parallel=5",
    "format:untracked": "npx nx format:write --untracked --parallel=5",
    "typecheck": "npx nx run-many --target=typecheck --parallel=5",
    "test:int": "npx nx run-many --target=test-int --parallel=5",
    "test:unit": "npx nx run-many --target=test-unit --parallel=5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@faker-js/faker": "^10.1.0",
    "@nx/esbuild": "^22.2.3",
    "@nx/eslint": "22.2.3",
    "@nx/eslint-plugin": "22.2.3",
    "@nx/express": "^22.2.3",
    "@nx/jest": "22.2.3",
    "@nx/js": "22.2.3",
    "@nx/node": "22.2.3",
    "@swc-node/register": "^1.11.1",
    "@swc/core": "^1.15.4",
    "@swc/helpers": "~0.5.17",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/mongoose": "^5.11.96",
    "@types/node": "25.0.1",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "esbuild": "^0.27.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-check-file": "^3.3.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.2.2",
    "jest": "^30.2.0",
    "jest-environment-node": "^30.2.0",
    "jest-util": "^30.2.0",
    "mongodb-memory-server": "^10.4.1",
    "nx": "22.2.3",
    "pino-pretty": "^13.1.3",
    "prettier": "^3.7.4",
    "ts-jest": "^29.4.6",
    "ts-node": "10.9.2",
    "tslib": "^2.8.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.49.0"
  },
  "dependencies": {
    "@ts-rest/core": "^3.52.1",
    "@ts-rest/express": "^3.52.1",
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "helmet": "^8.1.0",
    "mongoose": "^9.0.1",
    "pino": "^10.1.0",
    "pino-http": "^11.0.0",
    "stripe": "^20.0.0",
    "zod": "^3.25.76"
  }
}
